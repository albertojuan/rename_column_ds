DESCRIPTION >
	Round Time Down to minute


NODE signal_minute_0
SQL >

    SELECT 
        toStartOfMinute(time) AS time,
        network,
        coalesce(NULLIf(signal, ''), asset) AS asset,
        metric,
        value,
        unit,
        job_id,
        ingested_at
    FROM signals
    WHERE asset != ''

TYPE materialized
DATASOURCE signal_minute_mv
