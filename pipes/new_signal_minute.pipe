DESCRIPTION >
	Round Time Down to minute


NODE signal_minute_0
SQL >

    SELECT 
        toStartOfMinute(time) AS time,
        network,
        COALESCE(NULLIf(asset, ''), signal) AS signal,
        metric,
        value,
        unit,
        job_id,
        ingested_at
    FROM signals
    WHERE time > '2024-10-04 10:10:00'

TYPE materialized
DATASOURCE new_signal_minute_mv
ENGINE "MergeTree"
ENGINE_PARTITION_KEY "toYYYYMM(time)"
ENGINE_SORTING_KEY "time, metric, unit, job_id"
